<div class="container">

  <!-- Static navbar -->
  <div class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header {{{__ "logo"}}}">
        <a class="navbar-brand" href="../../"><img alt="إحصائيات التسجيل" src="./img/hnec_logo.png"></a>
      </div>
      <div class="navbar-collapse" dir="rtl" >
        <ul class="nav navbar-nav {{{__ "navbar"}}}">
          <li class="active"><a href="../../">{{{__ "main"}}}</a></li>
        </ul>
      </div><!--/.nav-collapse -->
    </div><!--/.container-fluid -->
  </div>
  
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="row-fluid">
        <table class="table">
          <tr>
            <td>
              <form role="form">
                <div class="form-group">
                  <label for="region">Region</label>
                  <select class="form-control" id="region">
                    {{#each centers}}
                    <option value="{{region}}">{{region}}</option>
                    {{/each}}
                  </select>
                </div>
              </form>
            </td>
            <td>
              <form role="form">
                <div class="form-group">
                  <label for="region">Offices</label>
                  <select class="form-control" id="offices">
                  </select>
                </div>
              </form>
            </td>
            <td>
              <form role="form">
                <div class="form-group">
                  <label for="exampleInputEmail1">SubConstituencies</label>
                  <select class="form-control" id = "subcons">
                    
                  </select>
                </div>
              </form>
            </td>
            <td>
              <form role="form">
                <div class="form-group">
                  <label for="mahallat">Village</label>
                  <select class="form-control" id = "mahallat">
                    
                  </select>
                </div>
              </form>
            </td>
            <td>
              <form role="form">
                <div class="form-group">
                  <label for="village">Mahalla</label>
                  <select class="form-control" id = "village">
                    
                  </select>
                </div>
              </form>
            </td>
          </tr>
        </table>
      </div>
      <div class="row-fluid">
        <div class="col-md-8">
          <div  id="map-canvas"></div>
        </div>
        <div class="table-responsive col-md-4" dir="{{{__ "dir"}}}">
            <table class="table table-striped table-bordered table-condensed">
              <thead>
                <tr>
                  <th>Center#</th>
                  <th>Center Name</th>
                </tr>
              </thead>
              <tbody id = "centers">
                
              </tbody>
            </table>
        </div>
      </div>
    </div>
  </div>
</div> <!-- /container -->


<script src="./js/jquery-1.10.2.min.js"></script>
<script src="./js/bootstrap.min.js"></script>
<script src="./js/purl.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
<script src="./js/index.js"></script>
